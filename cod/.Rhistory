# ---------------------------------------------
# Utilidades: evaluar componente y G(x)
# Admite g_j(x) (vector) o g_j(x1, ..., xn) (escalares)
# ---------------------------------------------
evaluar.componente <- function(gj, x) {
v <- try(gj(x), silent = TRUE)    # firma vectorial
if (!inherits(v, "try-error") && length(v) == 1 && is.finite(v)) return(as.numeric(v))
as.numeric(do.call(gj, as.list(x)))  # firma escalar
}
evaluar.G <- function(G, x) {
vapply(G, evaluar.componente, numeric(1), x = x)
}
# ---------------------------------------------
# Jacobiana numérica por diferencias centrales
# J_ij = d g_i / d x_j (x)
# ---------------------------------------------
calcula.jacobiano <- function(G, x, h = sqrt(.Machine$double.eps)) {
x <- as.numeric(x)
m <- length(x)
J <- matrix(0, nrow = m, ncol = m)
for (j in seq_len(m)) {
ej <- rep(0, m); ej[j] <- 1
fp <- evaluar.G(G, x + h * ej)
fm <- evaluar.G(G, x - h * ej)
J[, j] <- (fp - fm) / (2 * h)
}
J
}
# ---------------------------------------------
# Newton para varias variables (vector)
# P0    : punto inicial
# tol   : tolerancia (máx. norma-infinito del salto)
# n.max : iteraciones máximas
# G     : lista de funciones (g1, ..., gn)
# ---------------------------------------------
newton.vv <- function(G, P0, tol, n.max) {
x.prev <- as.numeric(P0)
for (i in seq_len(n.max)) {
J   <- calcula.jacobiano(G, x.prev)
Fx  <- evaluar.G(G, x.prev)
# Resolver J * delta = F(x)  (mejor que invertir)
delta <- try(solve(J, Fx), silent = TRUE)
if (inherits(delta, "try-error") || any(!is.finite(delta))) {
stop("Falla al resolver el sistema lineal: jacobiana singular o mal condicionada.")
}
x.next <- x.prev - as.numeric(delta)
if (max(abs(x.next - x.prev)) < tol) return(x.next)
x.prev <- x.next
}
message("Número máximo de iteraciones excedido")
return(x.prev)
}
f1 <- function(x1, x2) x1*(1-x1) + 4*x2 - 12
f2 <- function(x1, x2) (x1-2)^2 + (2*x2 - 3)^2 - 25
F  <- list(f1, f2)
p0 <- c(-1, 3.5)
sol <- newton.vv(G = F, p0, tol = 1e-10, n.max = 50)
sol
